{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Snowflake_Source",
						"type": "DatasetReference"
					},
					"name": "osEnquiries"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Snowflake_Target",
						"type": "DatasetReference"
					},
					"name": "ossinkTargetTable"
				}
			],
			"transformations": [],
			"scriptLines": [
				"parameters{",
				"     par_sourceTable as string,",
				"     par_targetTable as string",
				"}",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> osEnquiries",
				"osEnquiries sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true) ~> ossinkTargetTable"
			]
		}
	}
}